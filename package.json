{
  "name": "seneca-registry",
  "version": "0.2.1",
  "description": "Seneca service registry (simplistic single instance).",
  "main": "registry.js",
  "scripts": {
    "test": "lab -v -P test -L -t 90 -I",
    "coveralls": "lab -s -P test -r lcov | coveralls",
    "coverage": "lab -v -P test -L -t 90 -r html > docs/coverage.html",
    "annotate": "docco registry.js -o docs/annotated",
    "prettier": "prettier --write --no-semi --single-quote *.js lib/*.js test/*.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/senecajs/seneca-registry"
  },
  "keywords": [
    "seneca",
    "plugin",
    "service",
    "registry",
    "key",
    "value",
    "store"
  ],
  "author": "Richard Rodger (http://richardrodger.com)",
  "contributors": [
    "Richard Rodger (http://richardrodger.com)"
  ],
  "license": "MIT",
  "dependencies": {
    "lodash": "4.17.4"
  },
  "devDependencies": {
    "code": "4.1.x",
    "docco": "0.7.0",
    "lab": "13.1.x",
    "pre-commit": "1.2.x",
    "prettier": "1.4.x",
    "seneca": "plugin",
    "seneca-chain": "senecajs-labs/seneca-chain"
  },
  "files": [
    "LICENSE",
    "README.md",
    "registry.js"
  ],
  "pre-commit": [
    "prettier",
    "test",
    "annotate"
  ]
}
